										"type": "string"
									},
									"fieldType": {
										"type": "integer"
									},
									"fieldNumber": {
										"type": "integer"
									},
									"path": {
										"type": "string"
									}
								},
								"required": [
									"name",
									"kind",
									"fieldType"
								]
							}
						}
					}
				}
			}
