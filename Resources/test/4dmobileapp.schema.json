		"actions": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string"
					},
					"scope": {
						"type": "string"
					},
					"label": {
						"type": "string"
					},
					"shortLabel": {
						"type": "string"
					},
					"icon": {
						"type": "string"
					},
					"tableNumber": {
						"type": "integer"
					},
					"parameters": {
						"type": "array",
						"items": {
							"type": "object",
							"properties": {
								"name": {
									"type": "string"
								},
								"label": {
									"type": "string"
								},
								"shortLabel": {
									"type": "string"
								},
								"type": {
									"type": "string"
								}
							},
							"required": [
								"name",
								"type"
							]
						}
					}
				},
				"required": [
					"name",
					"scope"
				]
			}
		}
