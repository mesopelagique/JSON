		"ui": {
			"type": "object",
			"properties": {
				"dominantColor": {
					"type": "string"
				}
			}
		},
		"dataModel": {
			"type": "object",
			"patternProperties": {
				"[0-9]+": {
					"type": "object",
					"properties": {
						"": {
							"type": "object",
							"properties": {
								"name": {
									"type": "string"
								},
								"label": {
									"type": "string"
								},
								"shortLabel": {
									"type": "string"
								},
								"primaryKey": {
